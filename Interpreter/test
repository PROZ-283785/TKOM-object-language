funkcja(a in, b out, c in out, d)  extends obiekt{

    a = 1;
    b = 2;
    dodaj(a, b, c);
    d = ((a + -b) * (a + c));
    a = b * c.b;

    when( i == 1 ){
        out<<'1';
    } else when( i == 2 ){
        in>>a;
    } else {
        this.a = b;
    }

    loop( i = 1; i < 5; i = i + 1){
        a = b;
    }


    a = c + d + a + 2 + a.b;
    a = car.run(a in out);
    loop(a:b){
        in>>a;
    }
    out<< a + b + c;
}

dodaj(a in, b in, c out){
    c = a + b;

}
