main(){
    a = -(5 - 3 + 2 * 2 / 4 * (5 + 3 - 2));
    out<<a;
    truck = None;
    anotherTruck = None;
    base = 2;
    exponent = 6;
    sp1 = None;
    power(base, exponent, sp1);
    sp2 = 100;
    Truck(sp1, truck);
    Truck(sp2, anotherTruck);
    newTruck = truck + anotherTruck;
    newSpeed = None;
    newTruck.getSpeed(newSpeed);
    out<<newSpeed;
    out<<newTruck.speed;
    sum = 0;
    base = -2;
    loop( i = 0; i <= 5; i = i + 1){
        x = None;
        power(base, i, x);
        sum = sum + x;
    }
    out<<sum;
    when( truck < anotherTruck ){
        out<<'Wolniejszy';
    } else {
        out<<'Szybszy';
    }
    newTruck.setSpeed(sum);

}

